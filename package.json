{
  "name": "react-babel-webpack-boilerplate",
  "version": "1.1.0",
  "description": "react, babel, webpack, redux, react-router, unit test, eslint, material design",
  "main": "app/boostrap.jsx",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rhiokim/react-babel-webpack-boilerplate.git"
  },
  "scripts": {
    "lint": "eslint --format=node_modules/eslint-formatter-pretty 'app/**/*.@(js|jsx)'",
    "lint:msg": "validate-commit-msg",
    "test": "mocha --opts mocha.opts -R spec",
    "test:watch": "mocha --opts mocha.opts --watch -R min",
    "test:jenkins": "mocha --opts mocha.opts -R xunit > test-reports.xml",
    "test:jest": "jest",
    "test:karma": "karma start",
    "build": "webpack",
    "start": "webpack-dev-server --devtool eval --progress --hot --colors --content-base app",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "validate": "npm ls",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "docker-build": "docker build --tag react-app .",
    "docker-run": "docker run -d -p 80:80 -p 443:443 --name react-app react-app:latest",
    "docker-rm": "docker rm -f react-app"
  },
  "dependencies": {
    "axios": "^0.10.0",
    "babel-runtime": "^6.6.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "redux": "^3.5.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-jest": "^11.0.2",
    "babel-loader": "~6.2.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "commitizen": "^2.8.0",
    "conventional-changelog": "^1.1.0",
    "copy-webpack-plugin": "^2.1.1",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.1.6",
    "eslint": "^2.8.0",
    "eslint-config-xo": "^0.13.0",
    "eslint-config-xo-react": "^0.7.0",
    "eslint-formatter-pretty": "^0.2.0",
    "eslint-plugin-react": "^5.0.1",
    "expect": "^1.18.0",
    "expect-jsx": "^2.5.1",
    "file-loader": "^0.8.5",
    "ghooks": "^1.0.3",
    "install": "^0.6.1",
    "jest-cli": "^11.0.2",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^0.5.5",
    "karma-jenkins-reporter": "0.0.2",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "open-browser-webpack-plugin": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "react-addons-test-utils": "^15.0.1",
    "react-router-redux": "^4.0.2",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "style-loader": "~0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "~1.14.0"
  },
  "keywords": [
    "es6",
    "web app",
    "boilerplate"
  ],
  "author": "Ruan Yifeng",
  "license": "MIT",
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testFileExtensions": [
      "jsx"
    ],
    "testPathDirs": [
      "<rootDir>/app"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "ava": {
    "files": [
      "app/**/*-test.{js,jsx}"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
